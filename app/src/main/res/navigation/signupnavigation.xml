<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/signupnavigation"
    app:startDestination="@id/signupAFragment">

    <fragment
        android:id="@+id/signupAFragment"
        android:name="com.example.nongglenonggle.presentation.view.signup.SignupAFragment"
        android:label="SignupAFragment"
        tools:layout="@layout/fragment_signup_a">
        <action
            android:id="@+id/action_signupAFragment_to_signupBFragment"
            app:destination="@id/signupBFragment"
            app:popEnterAnim="@anim/from_left_anim"/>
    </fragment>
    <fragment
        android:id="@+id/signupBFragment"
        android:name="com.example.nongglenonggle.presentation.view.signup.SignupBFragment"
        android:label="SignupBFragment"
        tools:layout="@layout/fragment_signup_b">
        <action
            android:id="@+id/action_signupBFragment_to_signupCFragment"
            app:destination="@id/signupCFragment"
            app:popEnterAnim="@anim/from_left_anim"/>
    </fragment>
    <fragment
        android:id="@+id/signupCFragment"
        android:name="com.example.nongglenonggle.presentation.view.signup.SignupCFragment"
        android:label="SignupCFragment"
        tools:layout="@layout/fragment_signup_c">
        <action
            android:id="@+id/action_signupCFragment_to_signupDFragment"
            app:destination="@id/signupDFragment"
            app:popEnterAnim="@anim/from_left_anim"/>
    </fragment>
    <fragment
        android:id="@+id/signupDFragment"
        android:name="com.example.nongglenonggle.presentation.view.signup.SignupDFragment"
        android:label="SignupDFragment"
        tools:layout="@layout/fragment_signup_d">
        <action
            android:id="@+id/action_signupDFragment_to_addressSearchFragment"
            app:destination="@id/addressSearchFragment"
            app:popEnterAnim="@anim/from_left_anim"
            app:popExitAnim="@animator/nav_default_exit_anim"/>
    </fragment>
    <fragment
        android:id="@+id/addressSearchFragment"
        android:name="com.example.nongglenonggle.presentation.view.signup.AddressSearchFragment"
        android:label="AddressSearchFragment" >
        <action
            android:id="@+id/action_addressSearchFragment_to_signupDFragment"
            app:destination="@id/signupDFragment" />
    </fragment>
</navigation>